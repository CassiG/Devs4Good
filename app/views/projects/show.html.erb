<h2><%= @project.title %></h2>

<div>Project Details: <%= @project.description %></div>
<div>Requested By: <%= @project.time_frame %></div>

<% if logged_in? && current_user.user_type == 'org' %>
  <%= link_to('Edit', edit_project_path(@project)) %>
  <%= link_to('Destroy', @project, method: :delete, data: { confirm: 'Are you sure?' }) %>
<% end %>

<% if logged_in? && current_user.user_type == 'dev' %>
  <%= link_to('Add Proposal', new_project_proposal_path(@project)) %>
<% end %>
